---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>Huang's Blog</title>
    <link href="http://huang-x-h.github.io/"/>
    <link type="application/atom+xml" rel="self" href="http://huang-x-h.github.io/atom.xml"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://huang-x-h.github.io/</id>
    <author>
        <name>huangxinghui</name>
        <email>huang.x.hui@gmail.com</email>
    </author>

    {% for post in site.posts %}
    <entry>
        <id>{{site.url}}{{ post.id }}</id>
        <link type="text/html" rel="alternate" href="{{site.url}}{{ post.url }}"/>
        <title>{{ post.title }}</title>
        <published>{{ post.date | date_to_xmlschema }}</published>
        {% if page.last_modify_date %}
        <updated>{{ post.last_modify_date | date_to_xmlschema }}</updated>
        {% endif %}
        <author>
            <name>huangxinghui</name>
            <uri>http://huang-x-h.github.io/</uri>
        </author>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}

</feed>